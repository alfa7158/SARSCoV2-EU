<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.bookOfCovid.UploadVideosFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/uploadVideoButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="video" />

        <Button
            android:id="@+id/uplaodAudioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Audio" />

        <Button
            android:id="@+id/upload_pdfutton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PDF" />

        <Button
            android:id="@+id/UploadDocxButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="docx" />

        <Button
            android:id="@+id/uploadAPicBookOfCovid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="picture" />
    </LinearLayout>
</FrameLayout>